<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>titan</display-name>

	<context-param>
		<param-name>storage.backend</param-name>
		<param-value>cassandra</param-value>
	</context-param>
	<context-param>
		<param-name>storage.hostname</param-name>
		<param-value>fancy-center,fancy-data01,fancy-data02,fancy-data03,fancy-data04,fancy-data05</param-value>
	</context-param>
	<context-param>
		<param-name>storage.keyspace</param-name>
		<param-value>titan_relations_cloud</param-value>
	</context-param>


	<context-param>
		<param-name>redisHost</param-name>
		<!-- <param-value>183.61.12.145</param-value> -->
		<param-value>172.19.102.17</param-value>
	</context-param>
	<context-param>
		<param-name>redisPort</param-name>
		<param-value>6379</param-value>
	</context-param>
	<context-param>
		<param-name>redisDb</param-name>
		<param-value>1</param-value>
	</context-param>
	<context-param>
		<param-name>redisTimeout</param-name>
		<param-value>5000</param-value>
	</context-param>

</web-app>