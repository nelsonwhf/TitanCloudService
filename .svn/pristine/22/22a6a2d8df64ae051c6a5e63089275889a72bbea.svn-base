package com.duowan.yy.titan.cloud.service.vo;

public class SingleUserRecommReason {

	private Long uid;

	private Integer reason;// 推荐原因

	private String addition;

	/**
	 * 
	 */
	public SingleUserRecommReason() {
		super();
	}

	/**
	 * @param uid
	 * @param reason
	 * @param addition
	 */
	public SingleUserRecommReason(Long uid, Integer reason, String addition) {
		super();
		this.uid = uid;
		this.reason = reason;
		this.addition = addition;
	}

	/**
	 * @return the uid
	 */
	public Long getUid() {
		return uid;
	}

	/**
	 * @param uid
	 *            the uid to set
	 */
	public void setUid(Long uid) {
		this.uid = uid;
	}

	/**
	 * @return the reason
	 */
	public Integer getReason() {
		return reason;
	}

	/**
	 * @param reason
	 *            the reason to set
	 */
	public void setReason(Integer reason) {
		this.reason = reason;
	}

	/**
	 * @return the addition
	 */
	public String getAddition() {
		return addition;
	}

	/**
	 * @param addition
	 *            the addition to set
	 */
	public void setAddition(String addition) {
		this.addition = addition;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("SingleUserRecommReason [uid=").append(uid).append(", reason=")
				.append(reason).append(", addition=").append(addition).append("]");
		return builder.toString();
	}

}
